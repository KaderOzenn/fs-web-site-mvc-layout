                    <div class="demo-settings">
                        <p>Genel Ayarlar</p>
                        <ul class="setting-list">
                            <li>
                                <span>Kayıt Paneli</span>
                                <div class="switch">
                                    <label><input type="checkbox" checked><span class="lever"></span></label>
                                </div>
                            </li>
                            <li>
                                <span>Email Aktifliği</span>
                                <div class="switch">
                                    <label><input type="checkbox"><span class="lever"></span></label>
                                </div>
                            </li>
                        </ul>
                        <p>SİSTEM AYARLARI</p>
                        <ul class="setting-list">
                            <li>
                                <span>Bildirimler</span>
                                <div class="switch">
                                    <label><input type="checkbox" checked><span class="lever"></span></label>
                                </div>
                            </li>
                            <li>
                                <span>Otomatik Güncelleme</span>
                                <div class="switch">
                                    <label><input type="checkbox" checked><span class="lever"></span></label>
                                </div>
                            </li>
                        </ul>
                        <p>Giriş Ayarları</p>
                        <ul class="setting-list">
                            <li>
                                <span>Kapalı</span>
                                <div class="switch">
                                    <label><input type="checkbox"><span class="lever"></span></label>
                                </div>
                            </li>
                        </ul>
