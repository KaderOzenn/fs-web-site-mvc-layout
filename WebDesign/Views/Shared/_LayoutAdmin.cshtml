                    <img src="~/Content/Admin/images/user.png" width="48" height="48" alt="User" />

                </div>
                <div class="info-container">
                    <div class="name" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">Altan KIZGIN</div>
                    <div class="email">altankızgın@gmail.com</div>
                    <div class="btn-group user-helper-dropdown">
                        <i class="material-icons" data-toggle="dropdown" aria-haspopup="true" aria-expanded="true">keyboard_arrow_down</i>
                        <ul class="dropdown-menu pull-right">
                            <li><a href="javascript:void(0);"><i class="material-icons">person</i>Profil</a></li>
                            <li role="separator" class="divider"></li>
                            <li><a href="javascript:void(0);"><i class="material-icons">group</i>Takipçiler</a></li>

                            <li><a href="javascript:void(0);"><i class="material-icons">favorite</i>Beğeniler</a></li>
                            <li role="separator" class="divider"></li>
